Tab1Section:NewButton("Pls Capture", "Unlock All", function()
for i,v in pairs(game:GetService("Players").LocalPlayer.DataSave.Common.FillOnStart:GetDescendants()) do
    if v.ClassName == "BoolValue" then
        v.Value = true
    end
end
    print("Clicked")
end)
